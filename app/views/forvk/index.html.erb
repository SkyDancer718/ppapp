<!DOCTYPE html>
<html>
<head>
  <title>Ppapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= link_to "like", likes_path(user_id: 1), remote: true, method: :post %>
<h1>ЗАГОЛОВОК</h1>
<%@users.each do |user|%>
 <div class="user">
	 <%=image_tag(user[:photo_max], class: "img")%>
	 <%=user[:first_name]%>
	 <%=user[:last_name]%>
     <div class="navi_bar">
		 <div class="fake"></div>
		 <div class="like"></div>
		 <div class="add" ></div>
     </div>
 </div>
<%end%>

<div class="session_bar">
<%=image_tag(@cur_user[:photo_50], class: "img_cur")%>
<%= link_to @cur_user[:first_name] + @cur_user[:last_name], 
				"http://www.vk.com/id#{session[:vk_id]}", :target => "_blank" %>
</div>

</body>
</html>
