<!DOCTYPE html>
<html>
<head>
  <title>Ppapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%@users.each do |user|%>
<div class="logo">
<%=image_tag(user[:photo_max], class: "img")%>
<%=user[:first_name]%>
<%=user[:last_name]%>
</div>
<%end%>
</body>
</html>
